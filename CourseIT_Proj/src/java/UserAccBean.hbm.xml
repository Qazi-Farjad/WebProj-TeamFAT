<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="webProj.CourseIT.Beans.UserAccBean" table="UserAccInfo">
      <meta attribute="class-description">
         This class contains the Users Account details. 
          Password stored in plaintext, gugugugu.
          
      </meta>
      <id name="user_id" type="string" column="user_id">
      </id>
      <property name="name" column="name" type="string"/>
      <property name="email" column="email" type="string"/>
      <property name="password" column="password" type="string"/>
      <property name="user_pic" column="user_pic" type="string"/>
      <property name="internshipID" column="Internship" type="int"/>
   </class>
</hibernate-mapping>