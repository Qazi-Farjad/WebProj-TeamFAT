<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/dbcourseit</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">asdasd</property>
    <!-- List of XML mapping files -->
    <mapping class="webProj.CourseIT.Beans.Useraccinfo"/>
    <mapping class="webProj.CourseIT.Beans.Useracademicinfo"/>
    <mapping class="webProj.CourseIT.Beans.Review"/>
    <mapping class="webProj.CourseIT.Beans.Internships"/>
    <mapping class="webProj.CourseIT.Beans.Courses"/>
    <mapping class="webProj.CourseIT.Beans.Coursehistory"/>
    <mapping class="webProj.CourseIT.Beans.Courseenroll"/>
  </session-factory>
</hibernate-configuration>
