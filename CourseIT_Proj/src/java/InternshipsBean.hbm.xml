<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="webProj.CourseIT.Beans.InternshipsBean" table="Internships">
      <meta attribute="class-description">
         This class contains the Internships details. 
      </meta>
      <id name="internship_id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="submitter" column="submitter" type="string"/>
      <property name="info" column="info" type="string"/>
   </class>
</hibernate-mapping>